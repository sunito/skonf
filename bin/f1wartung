#!/bin/bash

logger "FFFFFFFFFFFFFFFFFFFFFFFFFFFFF start"

s=s
cmd=$s${s}h
echo $cmd

user=`cat $HOME/.$cmd/fernwartung/user`
echo $user

port=`grep "Port " /etc/ssh/sshd_config |cut -d" " -f2`
echo $port

ownport=$((10140 + `id -u`))
echo $ownport

logger "FFFFFFFFFFFFFF c$cmd o$ownport p$port u$user"

execute="$cmd -R $ownport:127.0.0.1:$port $user@suska.org"
logger "FFFFFFFFFFFFFFFFFFFFFFFFFFFFF vor execute"
logger "FFFFFF "$execute
echo $execute
$execute
logger "FFFFFFFFFFFFFFFFFFFFFFFFFFFFF fertig"

